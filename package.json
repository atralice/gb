{
  "name": "nextjs-starter-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port ${PORT:-3000}",
    "build": "next build",
    "start": "next start",
    "test": "./script/test-with-lock.sh",
    "db:seed:prod": "script/env-runner.sh production ./prisma/seed.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{css,json}": "prettier --write",
    "*.{ts,tsx}": "sh -c 'tsc -b --noEmit'",
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "7.3.0",
    "@radix-ui/react-icons": "1.3.2",
    "@tailwindcss/postcss": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "client-only": "0.0.1",
    "clsx": "2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "lodash": "^4.17.23",
    "lucide-react": "0.563.0",
    "next": "^16.1.6",
    "prisma": "7.3.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "server-only": "0.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/typography": "0.5.19",
    "@testing-library/react": "16.3.2",
    "@types/bun": "^1.3.8",
    "@types/lodash": "^4.17.23",
    "@types/node": "25.2.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "autoprefixer": "10.4.24",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-no-loops": "^0.4.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "fetch-mock": "12.6.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": "22"
  }
}
